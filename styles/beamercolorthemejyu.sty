\mode<presentation>

\definecolor{jyublue}{RGB}{0,41,87}
\definecolor{jyuorange}{RGB}{233,74,53}

% Settings
\setbeamercolor*{background canvas}{bg=white}
\setbeamercolor*{footer}{fg=white}
\setbeamercolor*{footer text}{fg=white}
\setbeamercolor*{title}{fg=white}
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{institute}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor*{frametitle}{fg=jyublue}
\setbeamercolor*{normal text}{fg=jyublue}
\setbeamercolor*{alerted text}{fg=jyuorange}
\setbeamercolor*{itemize item}{fg=jyuorange}
\setbeamercolor*{itemize subitem}{fg=jyuorange}
\setbeamercolor*{itemize subsubitem}{fg=jyuorange}
\setbeamercolor*{enumerate item}{fg=jyuorange}
\setbeamercolor*{enumerate subitem}{fg=jyuorange}
\setbeamercolor*{enumerate subsubitem}{fg=jyuorange}
\setbeamercolor*{description item}{fg=jyublue}
\setbeamercolor*{caption name}{fg=jyublue}
%Block
\setbeamercolor{block title}{bg=primaryLight, fg=white}%
\setbeamercolor{block body}{bg=white}%
\setbeamercolor{block title alerted}{bg=jyuorange, fg=white}%
\setbeamercolor{block body alerted}{bg=jyuorange!70}%
\setbeamercolor{block title example}{bg=jyuorange!80, fg=white}%
\setbeamercolor{block body example}{bg=jyublue!90!fg}%

% Bibliography
\setbeamercolor*{bibliography entry author}{fg=jyublue}
\setbeamercolor*{bibliography entry title}{fg=jyublue}
\setbeamercolor*{bibliography entry location}{fg=jyublue}
\setbeamercolor*{bibliography entry note}{fg=jyublue}
\setbeamercolor*{bibliograpgy text}{fg=jyuorange}
\def\@biblabel#1{\textcolor{jyuorange}{[#1]}}

\mode<all>