\mode<presentation>

\definecolor{primary}{RGB}{0,41,87}            % {RGB}{0,41,87}   = jyublue
\definecolor{secondary}{RGB}{233,74,53}        % {RGB}{233,74,53} = jyuorange

% Colours for text
\setbeamercolor*{background canvas}{bg=white}
\setbeamercolor*{footer}{fg=white}
\setbeamercolor*{footer text}{fg=white}
\setbeamercolor*{title}{fg=white}
\setbeamercolor*{title page header}{fg=white}
\setbeamercolor*{author}{fg=white}
\setbeamercolor*{institute}{fg=white}
\setbeamercolor*{date}{fg=white}
\setbeamercolor*{frametitle}{fg=primary}
\setbeamercolor*{normal text}{fg=primary}
\setbeamercolor*{alerted text}{fg=secondary}
\setbeamercolor*{itemize item}{fg=secondary}
\setbeamercolor*{itemize subitem}{fg=secondary}
\setbeamercolor*{itemize subsubitem}{fg=secondary}
\setbeamercolor*{enumerate item}{fg=secondary}
\setbeamercolor*{enumerate subitem}{fg=secondary}
\setbeamercolor*{enumerate subsubitem}{fg=secondary}
\setbeamercolor*{description item}{fg=primary}
\setbeamercolor*{caption name}{fg=primary}

%Block
\setbeamercolor{block title}{bg=primary, fg=white}%
\setbeamercolor{block body}{bg=white}%
\setbeamercolor{block title alerted}{bg=secondary, fg=white}%
\setbeamercolor{block body alerted}{bg=secondary!70}%
\setbeamercolor{block title example}{bg=secondary!80, fg=white}%
\setbeamercolor{block body example}{bg=primary!90!fg}%

% Bibliography
\setbeamercolor*{bibliography entry author}{fg=primary}
\setbeamercolor*{bibliography entry title}{fg=primary}
\setbeamercolor*{bibliography entry location}{fg=primary}
\setbeamercolor*{bibliography entry note}{fg=primary}
\setbeamercolor*{bibliograpgy text}{fg=secondary}
\def\@biblabel#1{\textcolor{secondary}{[#1]}}

\mode<all>